{
    "name": "hana",
    "version": "0.0.1",
    "license": "MIT",
    "scripts": {
        "build": "yarn run build:ts && yarn run build:docker",
        "build:ts": "ttsc",
        "build:docker": "docker build -t hancomac/hana:lastest -- ./",
        "run": "yarn node --es-module-specifier-resolution=node dist/index.js",
        "run:local": "yarn run build:ts && yarn run run",
        "run:docker": "yarn run build && docker run -p 80:80 -it hancomac/hana:lastest",
        "prettier": "prettier --write .",
        "release": "standard-version"
    },
    "dependencies": {
        "body-parser": "^1.19.1",
        "express": "^4.17.2",
        "express-ws": "^5.0.2",
        "serve-favicon": "^2.5.0",
        "typescript-is": "^0.19.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@commitlint/cli": "^16.0.1",
        "@commitlint/config-conventional": "^16.0.0",
        "@types/body-parser": "^1.19.2",
        "@types/express": "^4.17.13",
        "@types/express-ws": "^3.0.1",
        "@types/serve-favicon": "^2.5.3",
        "@types/uuid": "^8.3.4",
        "@types/ws": "^8.2.2",
        "husky": "^7.0.4",
        "prettier": "^2.5.1",
        "standard-version": "^9.3.2",
        "ttypescript": "^1.5.13",
        "typescript": "^4.5.4"
    },
    "packageManager": "yarn@3.1.1"
}
